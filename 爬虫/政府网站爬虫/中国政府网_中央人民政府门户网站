    //注册页面
    $('#userreg').click(function(){
      idm.user.openreg ({
        redirect:true,
        gourl: gourl,// 业务地址：非必填。回调总入口将以sp参数返回
        extargs: ''   // 扩展参数：非必填。回调将回传
      });
    })
    //登录页面
    $('#userlogin').click(function(){
      idm.user.openlogin ({
        redirect:true,
        gourl: gourl,// 业务地址：非必填。回调总入口将以sp参数返回
        extargs: ''   // 扩展参数：非必填。回调将回传
      });
    })
    
